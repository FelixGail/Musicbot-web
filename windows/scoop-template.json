{
  "homepage": "https://github.com/FelixGail/Musicbot-web",
  "description": "Web client for the Musicbot project",
  "version": "{{version}}",
  "url": "https://github.com/FelixGail/Musicbot-web/releases/download/{{version}}/musicbot-web-windows.exe",
  "hash": "sha512:{{hash}}",
  "installer": {
    "script": [
      "mkdir -p $dir\\config",
      "New-Item $dir\\config\\config.json",
      "Invoke-WebRequest -Uri \"https://raw.githubusercontent.com/FelixGail/Musicbot-web/master/packages/client/public/favicon.ico\" -OutFile \"$dir\\favicon.ico\""
    ]
  },
  "shortcuts": [
    ["musicbot-windows-web.exe", "MusicBot-web", "", "favicon.ico"]
  ],
  "persist": "config",
  "license": "MIT",
  "checkver": {
    "github": "https://github.com/FelixGail/Musicbot-web"
  },
  "autoupdate": {
    "url": "https://github.com/FelixGail/Musicbot-web/releases/download/$version/musicbot-web-windows.exe"
  }
}
